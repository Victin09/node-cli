{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "warn",
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off",
        "no-console": "off"
    }
}
