{
  "name": "express-app",
  "version": "1.0",
  "description": "Express boilerplate",
  "author": "Viti Gomez",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "precommit": "npm lint",
    "prestart": "yarn docs",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "lint:fix": "npm lint --fix",
    "lint:watch": "npm lint --watch",
    "dev": "ts-node-dev --respawn --rs --transpile-only src/index",
    "build": "tsc",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "es7",
    "es8",
    "es2017",
    "eslint",
    "coveralls",
    "boilerplate",
    "generator",
    "starter project"
  ],
  "dependencies": {
    "express": "^4.15.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.0",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "prettier": "^2.1.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
